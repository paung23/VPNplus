<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Application.AppInterface.NaiveBayesSpeedEval">

    <TextView android:id="@+id/nbse_tv"
        android:layout_width="match_parent"
        android:layout_height="400dip"
        android:scrollbars = "vertical"
        app:layout_constraintBottom_toTopOf="@+id/nbse_go_butt"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/nbse_go_butt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go"
        android:onClick="GO"
        app:layout_constraintTop_toBottomOf="@+id/nbse_tv"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">

    </Button>

</androidx.constraintlayout.widget.ConstraintLayout>